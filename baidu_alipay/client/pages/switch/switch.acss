.wrap {
    font-size: 16px;
}
.init-switch {
    margin: 0 05px 0 25px;
    vertical-align: middle;

}
.switch-text {
    vertical-align: -02px;
}
.item-wrap {
    background: #fff;
}
.item {
    padding: 15px 0;
    border-bottom: 1px #f5f5f5 solid;
    margin-left: 20px;
    position: relative;
}
.switch {
    position: absolute;
    top: 50%;
    right: 20px;
    transform: translateY(-50%); 
}
