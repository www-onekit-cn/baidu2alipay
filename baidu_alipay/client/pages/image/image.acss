.wrap {
    font-size: 16px;
}
.img-box {
    margin: auto 23px;
    position: relative;
    height: 113px;
}
.local-img {
    background: #EAEAEA;
}
.internet-img {
    background-color: #D2D3D4;
}
.swan-icon {
    width: 146px;
    height: 27px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
