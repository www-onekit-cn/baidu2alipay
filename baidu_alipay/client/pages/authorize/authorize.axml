<view  class="container">
    <view  class="page-body">
        <swan-button  a:for-item="item" a:key="item" a:for="{{items}}" id="{{item.id}}" onTap="btnClick" type="primary" hover-stop-propagation="true">{{item.name}}</swan-button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>